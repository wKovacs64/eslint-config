{
  "name": "@wkovacs64/eslint-config",
  "version": "7.8.0",
  "description": "@wKovacs64 ESLint config",
  "keywords": [
    "eslint",
    "eslintconfig",
    "eslintplugin",
    "eslint-config",
    "eslint-plugin",
    "wkovacs64"
  ],
  "author": {
    "name": "Justin Hall",
    "email": "justin.r.hall@gmail.com"
  },
  "license": "MIT",
  "type": "module",
  "main": "index.js",
  "exports": {
    ".": "./index.js",
    "./package.json": "./package.json"
  },
  "files": [
    "index.js"
  ],
  "homepage": "https://github.com/wKovacs64/eslint-config#readme",
  "repository": {
    "type": "git",
    "url": "https://github.com/wKovacs64/eslint-config.git"
  },
  "bugs": {
    "url": "https://github.com/wKovacs64/eslint-config/issues"
  },
  "scripts": {
    "changeset": "changeset",
    "changeset:version": "changeset version && npm install --package-lock-only",
    "changeset:publish": "changeset publish",
    "format": "prettier --cache --write .",
    "format:check": "prettier --cache --check .",
    "lint": "eslint .",
    "typecheck": "tsc"
  },
  "private": false,
  "prettier": "@wkovacs64/prettier-config",
  "publishConfig": {
    "access": "public"
  },
  "engines": {
    "node": "^18.18.0 || >=20.0.0"
  },
  "dependencies": {
    "@eslint/js": "^9.29.0",
